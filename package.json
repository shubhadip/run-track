{
  "name": "run-track",
  "version": "0.0.6",
  "description": "Run Track",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "scripts": {
    "cz": "git-cz",
    "start": "node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prepublishOnly": "yarn lint && yarn build",
    "commit-message": "yarn commitlint -e -V",
    "semantic-release:local": "yarn run build && yarn build:demo && semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^14.10.3",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-plugin-typescript": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/compiler-sfc": "^3.0.7",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-beta.7",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "express": "^4.17.1",
    "git-cz": "^4.7.4",
    "lint-staged": "^10.5.1",
    "postcss-color-function": "^4.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-discard-unused": "^4.0.1",
    "postcss-each": "^0.10.0",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "postcss-rem": "^1.1.5",
    "postcss-simple-vars": "^5.0.2",
    "postcss-sort-media-queries": "^1.6.24",
    "postcss-url": "^8.0.0",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "style-resources-loader": "^1.3.3",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.vue": "eslint --cache --fix"
  }
}
